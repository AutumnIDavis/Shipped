<div class="mainContainer">
      <div id="boatTable">
          <p id="notice"><%= notice %></p>
          <table>
            <thead>
              <tr>
                <th colspan="3"></th>
              </tr>
            </thead>
            <br>
          <div class="tableHeader">
              <%= link_to 'New Job', new_job_path %>
          </div>
            <br><br><br>
            <tbody>

              <% @jobs.each do |job| %>
            <br><br>
              <a href="jobs/<%= job.id %>">View: <%= job.name %></a>
        </div><br><br>

            	<div class="formFields">
                  Origin: <%= job.origin %> <br><br>
                  Destination: <%= job.destination %> <br><br>
        					Price: $<%= job.cost %> <br><br>
                	ID: <%= job.id %> <br><br>
              <% end %>
  </div>
